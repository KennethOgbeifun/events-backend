{
  "name": "backend",
  "version": "1.0.0",
  "main": "listen.js",
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "start": "node listen.js",
    "seed-dev": "NODE_ENV=development node db/seeds/run-seed.js",
    "seed-prod": "NODE_ENV=production npm run seed",
    "seed-test": "NODE_ENV=test node db/seeds/run-seed.js",
    "setup-dbs": "psql -d postgres -f ./db/setup.sql",
    "seed": "node ./db/seeds/run-seed.js",
    "dev": "nodemon listen.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Community Events API",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "googleapis": "^163.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "jest-extended": "^6.0.0",
    "jest-sorted": "^1.0.15",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.js",
      "jest-extended/all",
      "jest-sorted"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
